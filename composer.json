{
    "name": "joshfraser/php-name-parser",
    "description": "PHP library to split names into their respective components (first, last, etc)",
    "require": {
        "php": "^5.3.3 || ^7.0"
    },
    "autoload": {
        "psr-4": {
           "NameParser\\": "src/"
       }
    },
    "autoload-dev": {
       "psr-4": {
           "NameParser\\Test\\": "tests/"
       }
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
